&sk 		{ release-after-ms = <60000>; };
&caps_word	{ continue-list = <UNDERSCORE MINUS BACKSPACE K_CANCEL>; };

/ {
	// Sticky key on tap, normal key on hold
	// Useful for modifier-clicking without the modifier remaining active
	nk: not_so_sticky_key {
		compatible = "zmk,behavior-hold-tap";
		#binding-cells = <2>;
		tapping_term_ms = <250>;
		flavor = "tap-preferred";
		bindings = <&kp>, <&sk>;
	};

    skq: sticky_key_quick_release {
		compatible = "zmk,behavior-sticky-key";
		#binding-cells = <1>;
		bindings = <&kp>;
		quick-release;
		release-after-ms = <750>;
	};

	mm_sqt: mm_sqt_only {
		compatible = "zmk,behavior-mod-morph";
		#binding-cells = <0>;
		bindings = <&kp SQT>, <&kp SQT>;
		mods = <(MOD_LSFT|MOD_RSFT)>;
	};
	
	mm_left: mm_left_home {
		compatible = "zmk,behavior-mod-morph";
		#binding-cells = <0>;
		bindings = <&kp LEFT>, <&kp HOME>;
		mods = <(MOD_LALT|MOD_RALT)>;
	};
	
	mm_right: mm_right_end {
		compatible = "zmk,behavior-mod-morph";
		#binding-cells = <0>;
		bindings = <&kp RIGHT>, <&kp END>;
		mods = <(MOD_LALT|MOD_RALT)>;
	};

	mm_up: mm_up_pgup {
		compatible = "zmk,behavior-mod-morph";
		#binding-cells = <0>;
		bindings = <&kp UP>, <&kp PG_UP>;
		mods = <(MOD_LALT|MOD_RALT)>;
	};

	mm_down: mm_down_pgdn {
		compatible = "zmk,behavior-mod-morph";
		#binding-cells = <0>;
		bindings = <&kp DOWN>, <&kp PG_DN>;
		mods = <(MOD_LALT|MOD_RALT)>;
	};	
	

	conditional_layers {
		compatible = "zmk,conditional-layers";
		tri_layer {
			if-layers = <NAV NUM>;
			then-layer = <MISC>;
		};
	};
};